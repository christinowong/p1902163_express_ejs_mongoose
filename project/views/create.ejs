<html>
	<%-include('./head')%>
    <body style="padding-top:60px">
		<%-include('./header')%>
		<h1>Add a new apartment</h1>
		<form method="post" action="/createform">
			<div class="form-group">
				<label>Title:</label>
				<input name="title" value="" type="text">
			</div>
			<div class="form-group">
				<label>Apartment name:</label>
				<input name="aname" value="" type="text">
			</div>
			<div class="form-group">
				<label>Locate:</label>
				<input name="locate" value="" type="text">
			</div>
			<div class="form-group">
				<label>Rent:</label>
				<input name="rent" value="" type="text">
			</div>
			<div class="form-group">
				<label>Comment:</label>
				<textarea name="comment"></textarea>
			</div>
			<button type="submit">add</button>
		</form>
    </body>
</html>