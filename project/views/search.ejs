<html>
	<%-include('./head')%>
	<body style="padding-top:60px">
		<%-include('./header')%>
		<form method="post" action="/searchform">
			<select name="aname">
					<option value=""></option>
				<%for(data in content){%>
					<option value="<%=content[data].aname%>"><%-content[data].aname%></option>
				<%}%>
			</select>
			<button type="submit">search</button>
		</form>
		<%if (searchcontent){%>
			<% for(data in searchcontent){ %>
				<a href="/apartment?id=<%=searchcontent[data]._id%>"><div class="panel panel-default">
					<div class="panel-heading"><%-searchcontent[data].title%></div>
					<div class="panel-body">
						大廈名:<%-searchcontent[data].aname%></br>
						地址:<%-searchcontent[data].locate%></br>
						租:<%-searchcontent[data].rent%></br>
						附注:<%-searchcontent[data].comment%></br>
					</div>
				</div></a>
			<%}%>
		<%}else{%>
			<% for(data in content){ %>
				<a href="/apartment?id=<%=content[data]._id%>"><div class="panel panel-default">
					<div class="panel-heading"><%-content[data].title%></div>
					<div class="panel-body">
						大廈名:<%-content[data].aname%></br>
						地址:<%-content[data].locate%></br>
						租:<%-content[data].rent%></br>
						附注:<%-content[data].comment%></br>
					</div>
				</div></a>
			<%}%>
		<%}%>
	</body>
</html>